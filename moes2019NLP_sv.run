/*---------------------------------------------------------------------------------------------------------------------------------------
Load the main model and its data
---------------------------------------------------------------------------------------------------------------------------------------*/
model moes2019NLP_sv.mod;
data moes2019NLP_sv.dat;

/*---------------------------------------------------------------------------------------------------------------------------------------
AMPL options
---------------------------------------------------------------------------------------------------------------------------------------*/
/*option solver './snopt';*/
option solver snopt;
#option omit_zero_rows 1;
#option omit_zero_cols 1;

/*---------------------------------------------------------------------------------------------------------------------------------------
Solve the problem
---------------------------------------------------------------------------------------------------------------------------------------*/
solve;

/*---------------------------------------------------------------------------------------------------------------------------------------
Display commands (You can change them, of course)
---------------------------------------------------------------------------------------------------------------------------------------*/
display TC;

display dTminDC_1;

#display dTminDC_2; 

display TRadin;
display TDCout;
display AHEDC;
display dTLMDC;

display Qrad;
display E;
display TLMCond;
display Qevap;
display Qcond;
display COP;

display OPEX;
display CAPEX;


